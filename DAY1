CREATE DATABASE Company;
USE Company;
CREATE TABLE Employees(
ID INT,
Name VARCHAR(100),
Email_Address VARCHAR(100),
Department VARCHAR(100),
Date_of_Birth DATE,
Salary DECIMAL(10,2),
Is_Active BOOLEAN
);
